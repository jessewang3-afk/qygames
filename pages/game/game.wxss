.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 40rpx;
}
.board {
  display: flex;
  flex-direction: column;
}
.row {
  display: flex;
}
.cell {
  width: 80rpx;
  height: 80rpx;
  margin: 4rpx;
  border-radius: 8rpx;
  background: #fff;
  box-shadow: 0 2rpx 8rpx #ccc;
}
